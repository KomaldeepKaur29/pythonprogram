numbers = int(input("Enter the numbers of term? "))
n1, n2 = 0, 1
count = 0
if numbers <= 0:
    print("Please enter a positive integer")
elif numbers ==1:
    print("Fibonacci Series upto ",numbers,":")
    print(n1)
else:
    print("Fibonacci Sequence :")
    while count < numbers:
        print(n1)
        n = n1 + n2
        n1 = n2
        n2 = n
        count +=1
