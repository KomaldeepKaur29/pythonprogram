numlist = []
pos = []
num = int(input("please enter the element is list"))
for i in range(1,num+1):
    value = int(input(""))
    numlist.append(value)
print(numlist)
print("Positive numbers in the list are: ")
for j in range(num):
    if(numlist[j]>=0):
        print(numlist[j])
        pos.append(numlist[j])
print(pos)
